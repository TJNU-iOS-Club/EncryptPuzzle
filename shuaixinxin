#include<iostream>
using namespace std;
int main ()
{
	char c;
	int N;
	cout<<"N=";
	cin>>N;
	cout<<"c=";
	while((c=getchar())!='/n')
{   if ((c+N)>'Z')
	{if (c>='a')
	{if ((c+N)>'z')
	     c=(96+((((c+N)-'z')%26)));
	else c=(c+N);
	}
	else 
		c=(64+((((c+N)-'Z')%26)));
	}
	else c=c+N;
	
      cout<<c;  
	}
    cout<<endl;
	return 0;
}
